<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.diyorbek.dictionarybackend.mapper.TeatMapper">

    <select id="selectTestCnt" resultType="int" parameterType="map">
        select 1
        from dual
    </select>

    <select id="selectStudents" resultType="com.diyorbek.dictionarybackend.beans.StudentsBean" parameterType="map">
        select *
        from users
    </select>
</mapper>